.proMedal{
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  height: 72px;
  width: auto; 
}
.img{
  height: 64px;
  width: 55px;
  margin-left: auto;
  margin-right: auto;
}
.progress-bar{
  width: 40px;
  height: 5px;
  margin-left: auto;
  margin-right: auto;
  background-color: white;
  border-radius: 3px;
}
.progress{
  height: 100%;
  background-color: #ee548f;
  border-radius: 3px;
}